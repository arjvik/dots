#!/bin/bash

input="$(rofi -dmenu -sep ';' -p "qutebrowser" -theme-str 'listview { lines: 1; columns: 3; }' <<<" ;-T;-p")" || exit $?

set -- $input

if [[ $1 == -p ]]; then
	shift
	qutebrowser --target private-window "$@"
elif [[ $1 == -T ]]; then
	shift
	qutebrowser -T "$@"
else
	qutebrowser --target window "$@"
fi
