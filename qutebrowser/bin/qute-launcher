#!/bin/bash

sites=" ;voice.google.com;gmail.com;google.com;docs.google.com;github.com;usamts.org;:open -p"

input="`echo "$sites"|rofi -dmenu -sep ';' -l 1 -columns 8 -p "qutebrowser"`"

if [[ "$input" != "" ]]; then
	if [[ "$input" != " " ]]; then
		qutebrowser --target window "$input"
	else
		qutebrowser --target window
	fi
fi
